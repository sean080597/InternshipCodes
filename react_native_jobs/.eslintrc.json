{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "$": "readonly",
    "_": "readonly",
    "axios": "readonly",
    "process": "readonly",
    "require": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0,
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": [1, { "args": "after-used", "argsIgnorePattern": "^_" }],
    "no-useless-escape": 0
  }
}
